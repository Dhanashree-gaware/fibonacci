import java.util.Scanner;
class FibonacciSeries{
    public static void main(String[] args) {
        int a=0,b=1,c;
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter values:");
        int n=sc.nextInt();
        System.out.print(a+" "+b);
        for(int i=1;i<=n;i++)
        {
         c=a+b;
         System.out.print(" "+c);
        a=b;
        b=c;
        }
    }
} 
